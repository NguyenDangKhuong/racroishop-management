{
  "singleQuote": true,
  "jsxSingleQuote": true,
  "tabWidth": 2,
  "bracketSpacing": true,
  "bracketSameLine": true,
  "trailingComma": "none",
  "arrowParens": "avoid",
  "semi": false,
  "printWidth": 100,
  "importOrder": [
    "^(next/(.*)$)|^(next$)",
    "^(react/(.*)$)|^(react$)",
    "<THIRD_PARTY_MODULES>",
    "next-seo.config",
    "^src/components/(.*)$",
    "^src/utils/(.*)$",
    "^[./]",
    "^(@)(/.*|$)"
  ],
  "importOrderSeparation": true
}